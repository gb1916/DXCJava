<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.9.14.xsd">
<bean id="patient" class="com.org.health.Patient.java">
<property name="patientId" value="253"></property>
		<property name="patientName" value="Babitha"></property>
<property name="patientWeight" value="53.00"></property>
		<property name="patientHeight" value="5.2"></property>
</bean>
<bean id="BMI" class="com.org.health.BMICalculator.java">
<property name="patientWeight" value="53.00"></property>
		<property name="patientHeight" value="5.2"></property>
</bean>
</beans>